{
  "name": "advanced-blog-pet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=18.14.0"
  },
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js --port 8000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "eslint": "eslint \"**/*.{ts,tsx}\"",
    "eslint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:screenshots": "npx loki test",
    "test:screenshots:ok": "npx loki approve",
    "test:screenshots:ci": "brew install graphicsmagick && npx loki --requireReference update --reactUri file:./storybook-static",
    "storybook": "storybook dev -c ./config/storybook -p 6006",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
